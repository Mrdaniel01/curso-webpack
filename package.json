{
  "name": "curso-webpack",
  "version": "1.0.0",
  "description": "curso-webpack",
  "main": "index.js",
  "scripts": {
    "build": "webpack --entry ./index.js --output .bundle.js --mode development",
    "build:local": "webpack --mode production",
    "build:external": "webpack --config ./external/webpack.config.cualquiernombre.js",
    "build:multi": "webpack --config ./multi-entry/webpack.config.cualquiernombre.js",
    "build:css": "webpack --config ./css-loader/webpack.config.js",
    "build:plugins": "webpack --config ./plugins/webpack.config.js",
    "build:dev": "webpack-dev-server --config ./dev-server/webpack.config.js -w",
    "build:babel": "webpack-dev-server --config ./babel/webpack.config.js -w",
    "build:react": "webpack-dev-server --config ./react/webpack.config.js -w",
    "build:url": "webpack-dev-server --config ./url/webpack.config.js -w",
    "build:prepros": "webpack-dev-server --config ./prepros/webpack.config.js -w"
  },
  "keywords": [
    "webpack"
  ],
  "author": "Juan Rios",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "7.11.2",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@babel/core": "7.11.1",
    "@babel/plugin-transform-runtime": "7.11.0",
    "@babel/preset-env": "7.11.0",
    "@babel/preset-react": "7.10.4",
    "babel-loader": "8.1.0",
    "css-loader": "4.2.1",
    "file-loader": "6.0.0",
    "html-webpack-plugin": "4.3.0",
    "less": "3.12.2",
    "less-loader": "6.2.0",
    "mini-css-extract-plugin": "0.10.0",
    "node-sass": "4.14.1",
    "postcss-loader": "3.0.0",
    "sass-loader": "9.0.3",
    "style-loader": "1.2.1",
    "stylus": "0.54.8",
    "stylus-loader": "3.0.2",
    "url-loader": "4.1.0",
    "webpack": "4.44.1",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  }
}
